{
  "name": "roll_and_reserve",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed": "docker-compose exec web_server node_modules/.bin/ts-node src/seeds.ts",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:run": "npm run typeorm migration:run -- -d src/typeorm.config.ts",
    "migration:generate": "npm run typeorm migration:generate ./src/migrations/$npm_config_name -- -d src/typeorm.config.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^9.0.3",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.10",
    "@nestjs/mapped-types": "^2.0.0",
    "@nestjs/mongoose": "^11.0.1",
    "@nestjs/platform-express": "^11.0.10",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.0.5",
    "@nestjs/typeorm": "^11.0.0",
    "@pdfme/common": "^5.2.6",
    "@pdfme/generator": "^5.2.6",
    "axios": "^1.8.1",
    "bcryptjs": "^2.4.3",
    "bson": "5.5.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "file-type": "^16.5.3",
    "firebase-admin": "^13.0.2",
    "handlebars": "^4.7.8",
    "mongo-gridfs": "^1.1.0",
    "mongoose": "^7.4.3",
    "multer-gridfs-storage": "^5.0.2",
    "typeorm-extension": "^3.6.3",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "npm-force-resolutions": "^0.0.10",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "overrides": {
    "mongodb": "5.9.2",
    "bson": "5.5.1"
  }
}
